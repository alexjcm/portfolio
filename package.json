{
  "name": "porftolio-frontend",
  "version": "1.3.0",
  "description": "Starter React 17 with webpack",
  "scripts": {
    "build:dev": "webpack --mode=development --config webpack.dev.config.js",
    "build": "webpack --mode=production --config webpack.prod.config.js",
    "start": "webpack serve --config webpack.dev.config.js",
    "start2": "webpack-dev-server --config webpack.dev.config.js",
    "start3": "webpack serve --config webpack.dev.config.js",
    "start4": "webpack-dev-server --mode development --open --hot",
    "start5": "webpack serve --mode development",
    "docker-build": "docker build --rm -t portfolio-frontend .",
    "docker-run": "docker run --rm -v ${pwd}:/portfolio-frontend -p 3000:3000 --name portfolio-frontend-app portfolio-frontend",
    "docker-stop": "docker rm -f portfolio-frontend-app",
    "lint": "eslint --format=checkstyle",
    "lint2": "eslint src/**/*.js src/**/*.jsx --fix",
    "lint3": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "dependencies": {
    "TagCloud": "^2.3.2",
    "bootstrap": "^4.6.2",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.6",
    "react-dom": "^17.0.2",
    "react-github-calendar": "^3.4.0",
    "react-icons": "^4.7.1",
    "react-parallax-tilt": "^1.7.107",
    "react-router-dom": "^5.2.0",
    "react-tooltip": "^4.5.1",
    "react-tsparticles": "^2.9.3",
    "styled-components": "^5.3.6",
    "tsparticles": "^2.9.3",
    "typewriter-effect": "^2.19.0",
    "web-vitals": "^3.1.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-html-parser": "^6.8.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jasmine-core": "^4.5.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^5.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.4",
    "react-dev-utils": "^12.0.1",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
