{
  "name": "porftolio-frontend",
  "version": "2.3.1",
  "description": "Portfolio with React.js and Next.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "docker:build": "docker build -t alexjcm/portfolio -f docker/DockerfileNginxProd .",
    "docker:start": "docker run --rm -d -p 3000:80 --name portfolio alexjcm/portfolio",
    "test": "jest",
    "coverage": "jest --coverage",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\" --config ./.prettierrc.js",
    "lint": "eslint **/*.{js,jsx}",
    "lint2": "eslint **/*.{js,jsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx}",
    "lint:next": "next lint"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.0.1",
    "@sentry/nextjs": "^7.77.0",
    "bootstrap": "^4.6.2",
    "i18next": "^23.6.0",
    "next": "^14.0.1",
    "next-i18next": "^15.0.0",
    "pino": "^8.16.1",
    "react": "^18.2.0",
    "react-bootstrap": "^1.6.7",
    "react-dom": "^18.2.0",
    "react-github-calendar": "^4.0.1",
    "react-i18next": "^13.3.1",
    "react-icons": "^4.11.0",
    "react-parallax-tilt": "^1.7.172",
    "react-particles": "^2.12.2",
    "react-tooltip": "^5.22.0",
    "sharp": "^0.32.6",
    "styled-components": "^5.3.11",
    "TagCloud": "^2.4.0",
    "tsparticles": "^2.12.0",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.53.0",
    "eslint-config-next": "^14.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-html-parser": "^6.8.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "globals": "^13.23.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "react-dev-utils": "^12.0.1"
  }
}
