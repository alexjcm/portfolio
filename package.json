{
  "name": "porftolio-frontend",
  "version": "2.3.1",
  "description": "Portfolio with React.js and Next.js",
  "scripts": {
    "start:dev": "next dev",
    "start": "next start",
    "build": "next build",
    "docker:build": "docker build -t alexjcm/portfolio -f docker/DockerfileNginxProd .",
    "docker:start": "docker run --rm -d -p 3000:80 --name portfolio alexjcm/portfolio",
    "test": "jest",
    "coverage": "jest --coverage",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\" --config ./.prettierrc.js",
    "lint": "eslint **/*.{js,jsx}",
    "lint2": "eslint **/*.{js,jsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx}",
    "lint:next": "next lint"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.1.0",
    "@sentry/nextjs": "^7.94.1",
    "@tsparticles/react": "^3.0.0",
    "bootstrap": "^4.6.2",
    "i18next": "^23.7.16",
    "next": "^14.1.0",
    "next-i18next": "^15.2.0",
    "pino": "^8.17.2",
    "react": "^18.2.0",
    "react-bootstrap": "^1.6.7",
    "react-dom": "^18.2.0",
    "react-github-calendar": "^4.1.1",
    "react-i18next": "^14.0.1",
    "react-icons": "^5.0.1",
    "react-parallax-tilt": "^1.7.191",
    "react-tooltip": "^5.25.2",
    "sharp": "^0.33.2",
    "styled-components": "^5.3.11",
    "TagCloud": "^2.4.0",
    "tsparticles": "^3.1.0",
    "typewriter-effect": "^2.21.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-html-parser": "^6.8.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "globals": "^13.24.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.4",
    "react-dev-utils": "^12.0.1"
  }
}
