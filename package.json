{
  "name": "porftolio-frontend",
  "version": "1.4.1",
  "description": "Portfolio with React 17 and webpack",
  "scripts": {
    "start": "webpack serve --config webpack.dev.config.js",
    "build": "webpack --config webpack.prod.config.js",
    "build:dev": "webpack --config webpack.dev.config.js",
    "docker:build": "docker build -t alexjcm/portfolio -f docker/DockerfilePro .",
    "docker:start": "docker run --rm -d -p 3000:80 --name portfolio alexjcm/portfolio",
    "test": "jest",
    "coverage": "jest --coverage",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\" --config ./.prettierrc.js",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx}",
    "analyze": "source-map-explorer 'build/*.js' --html ./build/result.html"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "dependencies": {
    "TagCloud": "^2.3.2",
    "bootstrap": "^4.6.2",
    "react": "^18.2.0",
    "react-bootstrap": "^1.6.6",
    "react-dom": "^18.2.0",
    "react-github-calendar": "^3.4.0",
    "react-icons": "^4.8.0",
    "react-parallax-tilt": "^1.7.114",
    "react-router-dom": "^5.3.4",
    "react-tooltip": "^4.5.1",
    "react-tsparticles": "^2.9.3",
    "styled-components": "^5.3.8",
    "tsparticles": "^2.9.3",
    "typewriter-effect": "^2.19.0",
    "web-vitals": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-html-parser": "^6.8.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "file-loader": "^6.2.0",
    "globals": "^13.20.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.3",
    "prettier": "^2.8.4",
    "react-dev-utils": "^12.0.1",
    "source-map-explorer": "^2.5.3",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.7",
    "url-loader": "^4.1.1",
    "webpack": "^5.76.1",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
