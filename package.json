{
  "name": "porftolio-frontend",
  "version": "2.3.2",
  "description": "Portfolio with React.js and Next.js",
  "scripts": {
    "start": "next start",
    "start:dev": "next dev --turbopack",
    "build": "next build",
    "docker:build": "docker build -t alexjcm/portfolio -f docker/DockerfileNginxProd .",
    "docker:start": "docker run --rm -d -p 3000:80 --name portfolio alexjcm/portfolio",
    "test": "jest",
    "coverage": "jest --coverage",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\" --config ./.prettierrc.js",
    "lint": "eslint **/*.{js,jsx}",
    "lint2": "eslint **/*.{js,jsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx}",
    "lint:next": "next lint"
  },
  "dependencies": {
    "@sentry/nextjs": "^8.47.0",
    "@tsparticles/basic": "^3.7.1",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.7.1",
    "bootstrap": "^5.3.3",
    "i18next": "^24.2.0",
    "next": "15.1.3",
    "next-i18next": "^15.4.1",
    "pino": "^9.6.0",
    "react": "19.0.0",
    "react-bootstrap": "^2.10.7",
    "react-dom": "19.0.0",
    "react-github-calendar": "^4.5.2",
    "react-i18next": "^15.4.0",
    "react-icons": "^5.4.0",
    "react-parallax-tilt": "^1.7.269",
    "react-tooltip": "^5.28.0",
    "sharp": "^0.33.5",
    "styled-components": "^6.1.13",
    "TagCloud": "^2.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "15.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-html-parser": "^6.8.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "globals": "^15.14.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.4.2",
    "react-dev-utils": "^12.0.1"
  }
}
