{
  "name": "porftolio-frontend",
  "version": "1.2.0",
  "private": true,
  "dependencies": {
    "TagCloud": "^2.3.2",
    "bootstrap": "^4.6.2",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.6",
    "react-dom": "^17.0.2",
    "react-github-calendar": "^3.4.0",
    "react-icons": "^4.7.1",
    "react-parallax-tilt": "^1.7.90",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-tooltip": "^5.8.1",
    "react-tsparticles": "^2.9.3",
    "styled-components": "^5.3.6",
    "tsparticles": "^2.9.3",
    "typewriter-effect": "^2.19.0",
    "web-vitals": "^3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "docker-build": "docker build --rm -t portfolio-frontend .",
    "docker-run": "docker run --rm -v ${pwd}:/portfolio-frontend -p 3000:3000 --name portfolio-frontend-app portfolio-frontend",
    "docker-stop": "docker rm -f portfolio-frontend-app",
    "lint": "eslint --format=checkstyle",
    "lint2": "eslint src/**/*.{ts,tsx} --quiet --fix",
    "lint3": "eslint src/**/*.js src/**/*.jsx --fix",
    "lint4": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^12.8.3",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-html-parser": "^6.8.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jasmine-core": "^4.5.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^5.1.0",
    "prettier": "^2.8.4",
    "retire": "^3.2.3"
  }
}
