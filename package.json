{
  "name": "porftolio-frontend",
  "version": "2.3.0",
  "description": "Portfolio with React.js and Next.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "docker:build": "docker build -t alexjcm/portfolio -f docker/DockerfileNginxProd .",
    "docker:start": "docker run --rm -d -p 3000:80 --name portfolio alexjcm/portfolio",
    "test": "jest",
    "coverage": "jest --coverage",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\" --config ./.prettierrc.js",
    "lint": "eslint **/*.{js,jsx}",
    "lint2": "eslint **/*.{js,jsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx}",
    "lint:next": "next lint"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.4.10",
    "@sentry/nextjs": "^7.59.2",
    "bootstrap": "^4.6.2",
    "i18next": "^23.2.11",
    "next": "^13.4.10",
    "next-i18next": "^14.0.0",
    "pino": "^8.14.1",
    "react": "^18.2.0",
    "react-bootstrap": "^1.6.7",
    "react-dom": "^18.2.0",
    "react-github-calendar": "^4.0.1",
    "react-i18next": "^13.0.2",
    "react-icons": "^4.10.1",
    "react-parallax-tilt": "^1.7.154",
    "react-particles": "^2.11.0",
    "react-tooltip": "^5.18.1",
    "sharp": "^0.32.3",
    "styled-components": "^5.3.11",
    "TagCloud": "^2.3.2",
    "tsparticles": "^2.11.0",
    "typewriter-effect": "^2.20.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "babel-jest": "^29.6.1",
    "eslint": "^8.45.0",
    "eslint-config-next": "13.4.10",
    "eslint-config-prettier": "^8.8.0",
    "eslint-html-parser": "^6.8.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "globals": "^13.20.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "prettier": "^3.0.0",
    "react-dev-utils": "^12.0.1"
  }
}
