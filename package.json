{
  "name": "porftolio-frontend",
  "version": "2.0.0",
  "description": "Portfolio with React.js and Next.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "docker:build": "docker build -t alexjcm/portfolio -f docker/DockerfileNginxProd .",
    "docker:start": "docker run --rm -d -p 3000:80 --name portfolio alexjcm/portfolio",
    "test": "jest",
    "coverage": "jest --coverage",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\" --config ./.prettierrc.js",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx}",
    "lint:next": "next lint"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.3.0",
    "TagCloud": "^2.3.2",
    "bootstrap": "^4.6.2",
    "next": "^13.3.0",
    "pino": "^8.11.0",
    "react": "^18.2.0",
    "react-bootstrap": "^1.6.6",
    "react-dom": "^18.2.0",
    "react-github-calendar": "^3.4.0",
    "react-icons": "^4.8.0",
    "react-parallax-tilt": "^1.7.114",
    "react-particles": "^2.9.3",
    "react-tooltip": "^4.5.1",
    "sharp": "^0.32.0",
    "styled-components": "^5.3.8",
    "tsparticles": "^2.9.3",
    "typewriter-effect": "^2.19.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "babel-jest": "^29.5.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-html-parser": "^6.8.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "globals": "^13.20.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.4",
    "react-dev-utils": "^12.0.1"
  }
}
